// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_DB_CONN")
}

model session {
  sid    String   @unique
  sess   Json
  expire DateTime
}

model User {
  id        String   @id @map("id")
  email     String   @unique
  password  String?
  createdAt DateTime
  updatedAt DateTime
}
